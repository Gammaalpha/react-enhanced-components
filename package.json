{
  "name": "react-enhanced-components",
  "version": "1.2.0",
  "private": true,
  "homepage": "https://gammaalpha.github.io/react-enhanced-components",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.8",
    "@testing-library/user-event": "^12.1.1",
    "@types/jest": "^26.0.9",
    "@types/node": "^14.0.27",
    "@types/react-color": "^3.0.4",
    "core-js": "3",
    "quill": "^1.3.7",
    "react": "^16.13.1",
    "react-color": "^2.18.1",
    "react-dom": "^16.13.1",
    "react-quill": "^2.0.0-beta.2",
    "react-scripts": "3.4.2"
  },
  "scripts": {
    "publish": "npm run build-wp && sh deploy.sh",
    "tsCopy": "cp -f ./tsconfig.prod.json ./tsconfig.json",
    "webpack:dev": "npm run tsCopy && npx webpack --config webpack.config.js",
    "build-wp": "rm -rf build/ && npm run tsCopy && webpack --mode production",
    "watch": "npm run tsCopy && webpack --config webpack.config.js --mode production --watch",
    "deploy-gh": "rm -rf build/ && npm run build && gh-pages -d build && sh gh-deploy.sh",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "build/index.js",
  "types": "build/TestComponent.d.ts",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/quill": "^2.0.3",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.1",
    "css-modules-typescript-loader": "^4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "gh-pages": "^3.1.0",
    "inline-source-map": "^0.6.2",
    "mini-css-extract-plugin": "^0.10.0",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "source-map": "^0.7.3",
    "source-map-loader": "^1.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "keywords": [
    "React",
    "Material UI",
    "Data Table",
    "Rich Text Editor"
  ],
  "author": "Faiq Malik",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gammaalpha/react-enhanced-components.git"
  },
  "bugs": {
    "url": "https://github.com/Gammaalpha/react-enhanced-components/issues"
  }
}
